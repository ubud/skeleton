{
    "name": "ubud-skeleton",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "lerna:bootstrap": "lerna bootstrap --hoist --nohoist=@ubud/*",
        "build:libs": "run-p build:libs:*",
        "build:libs:bootstrap-theme": "ncp ./src/@ubud/bootstrap-theme ./dist/bootstrap-theme",
        "build:libs:dropdown": "ng-packagr -p ./src/@ubud/dropdown/package.json",
        "build:libs:menus": "ng-packagr -p ./src/@ubud/menus/package.json",
        "build:libs:ngrx": "ng-packagr -p ./src/@ubud/ngrx/package.json",
        "build:libs:template": "ng-packagr -p ./src/@ubud/template/package.json",
        "build:libs:tooltip": "ng-packagr -p ./src/@ubud/tooltip/package.json",
        "build:libs:user-control": "ng-packagr -p ./src/@ubud/user-control/package.json",
        "build:libs:utilities": "ng-packagr -p ./src/@ubud/utilities/package.json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.2.0",
        "@angular/common": "^5.2.0",
        "@angular/compiler": "^5.2.0",
        "@angular/core": "^5.2.0",
        "@angular/forms": "^5.2.0",
        "@angular/http": "^5.2.0",
        "@angular/platform-browser": "^5.2.0",
        "@angular/platform-browser-dynamic": "^5.2.0",
        "@angular/router": "^5.2.0",
        "@angular/service-worker": "^5.2.0",
        "@ngrx/effects": "~5.2.0",
        "@ngrx/router-store": "~5.2.0",
        "@ngrx/store": "~5.2.0",
        "@ngrx/store-devtools": "~5.2.0",
        "@ubud/bootstrap-theme": "^1.0.0",
        "@ubud/dropdown": "^1.0.0",
        "@ubud/menus": "^1.0.0",
        "@ubud/ngrx": "^1.0.0",
        "@ubud/template": "^1.0.0",
        "@ubud/tooltip": "^1.0.0",
        "@ubud/user-control": "^1.0.0",
        "@ubud/utilities": "^1.0.0",
        "core-js": "^2.4.1",
        "rxjs": "^5.5.6",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@angular/cli": "~1.7.2",
        "@angular/compiler-cli": "^5.2.0",
        "@angular/language-service": "^5.2.0",
        "codelyzer": "^4.0.1",
        "ncp": "^2.0.0",
        "ng-packagr": "^2.2.0",
        "npm-run-all": "^4.1.2",
        "tslint": "~5.9.1",
        "tslint-config-airbnb": "~5.7.0",
        "typescript": "~2.5.3"
    }
}
