{
    "name": "ubud-skeleton",
    "version": "1.1.10",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "lerna:bootstrap": "lerna bootstrap --hoist --nohoist=@ubud/*",
        "lerna:publish": "lerna publish --independent --skip-npm && npm run publish:libs",
        "publish:libs": "run-p publish:libs:*",
        "publish:libs:bootstrap-theme": "npm publish dist/bootstrap-theme --access public",
        "publish:libs:dropdown": "npm publish dist/dropdown --access public",
        "publish:libs:form": "npm publish dist/form --access public",
        "publish:libs:http": "npm publish dist/http --access public",
        "publish:libs:loader": "npm publish dist/loader --access public",
        "publish:libs:menus": "npm publish dist/menus --access public",
        "publish:libs:ngrx": "npm publish dist/ngrx --access public",
        "publish:libs:storage": "npm publish dist/storage --access public",
        "publish:libs:template": "npm publish dist/template --access public",
        "publish:libs:tooltip": "npm publish dist/tooltip --access public",
        "publish:libs:user-control": "npm publish dist/user-control --access public",
        "publish:libs:utilities": "npm publish dist/utilities --access public"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.2.0",
        "@angular/common": "^5.2.0",
        "@angular/compiler": "^5.2.0",
        "@angular/core": "^5.2.0",
        "@angular/forms": "^5.2.0",
        "@angular/http": "^5.2.0",
        "@angular/platform-browser": "^5.2.0",
        "@angular/platform-browser-dynamic": "^5.2.0",
        "@angular/router": "^5.2.0",
        "@angular/service-worker": "^5.2.0",
        "@ionic/storage": "^2.1.3",
        "@ngrx/effects": "~5.2.0",
        "@ngrx/router-store": "~5.2.0",
        "@ngrx/store": "~5.2.0",
        "@ngrx/store-devtools": "~5.2.0",
        "@ubud/bootstrap-theme": "^1.1.6",
        "@ubud/dropdown": "^1.1.6",
        "@ubud/menus": "^1.1.6",
        "@ubud/ngrx": "^1.1.8",
        "@ubud/template": "^1.1.6",
        "@ubud/tooltip": "^1.1.7",
        "@ubud/user-control": "^1.1.6",
        "@ubud/utilities": "^1.1.6",
        "core-js": "^2.4.1",
        "rxjs": "^5.5.6",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@angular/cli": "~1.7.2",
        "@angular/compiler-cli": "^5.2.0",
        "@angular/language-service": "^5.2.0",
        "@types/qs": "~6.5.1",
        "codelyzer": "^4.0.1",
        "ncp": "^2.0.0",
        "ng-packagr": "^2.2.0",
        "npm-run-all": "^4.1.2",
        "rmdir-cli": "^2.0.6",
        "tslint": "~5.9.1",
        "tslint-config-airbnb": "~5.7.0",
        "typescript": "~2.5.3"
    }
}
